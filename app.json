{
  "expo": {
    "name": "Frontend",
    "slug": "Frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://nyabyte.dev/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Frontend"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "https://nyabyte.dev/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Frontend"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "https://nyabyte.dev/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "https://nyabyte.dev/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
